    "hash": "0eef827ea868feea4ff0143a599b0482ccdd882e5acbc3c5018135313773867f",
    "extract_to": "IDE",
    "installer": {
        "script": "& \"$bucketsdir\\versions\\scripts\\jetbrains\\portable.ps1\" $dir $persist_dir"
