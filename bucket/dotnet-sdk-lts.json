    "bin": "dotnet.exe",
    "suggest": {
        "Visual C++ Redistributable": "extras/vcredist2019"
    },
    "env_add_path": ".",
    "env_set": {
        "DOTNET_ROOT": "$dir",
        "MSBuildSDKsPath": "$dir\\sdk\\$version\\Sdks"
