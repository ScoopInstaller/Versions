            "hash": "8fd8fcb8ff68afcfff759ac6653d873b5c0e8df3766a353641bad5274ba9f7bd"
        },
        "32bit": {
            "url": "https://dl.google.com/release2/chrome/adrqk5zr4tyfxgn2w4cxwft6eq7a_121.0.6152.0/121.0.6152.0_chrome_installer.exe#/dl.7z",
            "hash": "2074c670786cc500d3da074ef99c335c2ebdeba8d7862a33bf7dfa88647858fb"
        }
    },
    "installer": {
        "script": "Expand-7zipArchive \"$dir\\chrome.7z\" -ExtractDir 'Chrome-bin' -Removal"
