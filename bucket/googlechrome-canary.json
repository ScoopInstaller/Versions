            "hash": "fdc665d3089995cb1fba0e4030d7181fb19ad602624e5984c1984064f3f706f1"
        },
        "32bit": {
            "url": "https://dl.google.com/release2/chrome/adpshjopqc4j3hptg62v7iymemra_107.0.5265.0/107.0.5265.0_chrome_installer.exe#/dl.7z",
