    "hash": "540fa5ac764d38b3e49c536a67f74b1f6e8825b03716f2945ac9d93c90b787c1",
    "extract_to": "IDE",
    "installer": {
        "script": "& \"$bucketsdir\\versions\\scripts\\jetbrains\\portable.ps1\" $dir $persist_dir"
