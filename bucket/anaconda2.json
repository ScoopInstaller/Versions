{
    "version": "2019.07",
    "license": "BSD-3-Clause",
    "homepage": "https://www.anaconda.com/",
    "checkver": {
        "url": "https://repo.continuum.io/archive",
        "re": "Anaconda2-([\\d.]+)-Windows"
    },
    "bin": [
        "python.exe",
        "pythonw.exe",
        [
            "python.exe",
            "python2"
        ]
    ],
    "pre_install": "Write-Host 'Installing Anaconda 2. This can take up to 30 minutes on an HDD.' -ForegroundColor Magenta",
    "installer": {
        "args": [
            "/S",
            "/InstallationType=JustMe",
            "/AddToPath=0",
            "/D=$dir"
        ]
    },
    "uninstaller": {
        "file": "Uninstall-Anaconda2.exe",
        "args": "/S"
    },
    "persist": "envs",
    "env_add_path": "Scripts",
    "architecture": {
        "64bit": {
            "url": "https://repo.continuum.io/archive/Anaconda2-2019.07-Windows-x86_64.exe",
            "hash": "fb7493a5c40d28ab47e54c57f025186dc26309183b21c8a0df733837e86b1aad"
        },
        "32bit": {
            "url": "https://repo.continuum.io/archive/Anaconda2-2019.07-Windows-x86.exe",
            "hash": "1bd676a51ccdee57c2c01a2bc87fa8b1bd48cf7a6c0b16c44a241374f798f919"
        }
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://repo.continuum.io/archive/Anaconda2-$version-Windows-x86_64.exe"
            },
            "32bit": {
                "url": "https://repo.continuum.io/archive/Anaconda2-$version-Windows-x86.exe"
            }
        },
        "hash": {
            "url": "http://docs.anaconda.com/anaconda/install/hashes/$basename-hash/",
            "regex": "([A-Fa-f0-9]{64})"
        }
    }
}
