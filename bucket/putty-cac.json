            },
            "32bit": {
                "url": "https://raw.githubusercontent.com/NoMoreFood/putty-cac/$version/binaries/puttycac-$version.zip"
            }
        },
        "hash": {
            "url": "$baseurl/puttycac-hash.txt",
            "regex": "$sha256.*?$basename"
        }
    }
