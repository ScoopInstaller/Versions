    "architecture": {
        "64bit": {
            "url": "https://raw.githubusercontent.com/NoMoreFood/putty-cac/0.78u1/binaries/puttycac-64bit-0.78u1.zip",
            "hash": "23a5445f9d3ae199c826b71b3b822ab30204741239fb1f8df40ae2748db91653"
        },
        "32bit": {
            "url": "https://raw.githubusercontent.com/NoMoreFood/putty-cac/0.78u1/binaries/puttycac-0.78u1.zip",
            "hash": "75f1315888f89404b3ef18a8f2e459ab0a508027df357e007e3a29eb8c31c242"
        }
    },
    "bin": [
        "putty.exe",
        "puttygen.exe",
        "pscp.exe",
        "pageant.exe",
        "psftp.exe",
        "plink.exe",
        "puttytel.exe"
    ],
    "shortcuts": [
        [
            "putty.exe",
            "PuTTY"
        ],
        [
            "pageant.exe",
            "Pageant"
        ],
        [
            "psftp.exe",
