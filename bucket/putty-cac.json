        ],
        [
            "puttygen.exe",
            "PuTTYgen"
        ],
        [
            "puttytel.exe",
            "PuTTYtel"
        ]
    ],
    "checkver": {
        "github": "https://github.com/NoMoreFood/putty-cac",
        "regex": "/releases/tag/([\\w.]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
