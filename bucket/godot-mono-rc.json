{
    "version": "4.0.1-rc2",
    "description": "A feature-packed, cross-platform game engine to create 2D and 3D games from a unified interface.",
    "homepage": "https://godotengine.org/",
    "license": "MIT",
    "architecture": {
        "64bit": {
            "url": "https://downloads.tuxfamily.org/godotengine/4.0.1/rc2/mono/Godot_v4.0.1-rc2_mono_win64.zip",
            "hash": "sha512:61576b9d17794c165cd53e253827282c963d18503883582a8647493254f4f274e2124500b974a36b8e1072fae25e5944aaf1ee7189838d78c8a43b437b1ef884",
            "extract_dir": "Godot_v4.0.1-rc2_mono_win64"
        },
        "32bit": {
            "url": "https://downloads.tuxfamily.org/godotengine/4.0.1/rc2/mono/Godot_v4.0.1-rc2_mono_win32.zip",
            "hash": "sha512:989e68910475c57d6f9dd2c922a2a3411f8cd58c0bb4229a806043b149046e0a55e03c75395016dddc763729a21b757fd1cb2cd560882bc47e5f1211b3ba88c4",
            "extract_dir": "Godot_v4.0.1-rc2_mono_win32"
        }
    },
    "pre_install": [
        "Remove-Item \"$dir\\Godot_*_console.*\"",
        "Get-Item \"$dir\\Godot_*.exe\" | Rename-Item -NewName 'godot-mono.exe'"
    ],
    "bin": "godot-mono.exe",
    "shortcuts": [
        [
            "godot-mono.exe",
            "Godot Engine (Mono)"
        ]
    ],
    "checkver": {
        "url": "https://godotengine.org/blog/pre-release/",
        "regex": "Release candidate: Godot (?<ver>[\\d.]+) RC (?<rc>[\\d.]+)",
        "replace": "${1}-rc${2}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://downloads.tuxfamily.org/godotengine/$matchVer/$preReleaseVersion/mono/Godot_v$version_mono_win64.zip",
                "extract_dir": "Godot_v$version_mono_win64"
            },
            "32bit": {
                "url": "https://downloads.tuxfamily.org/godotengine/$matchVer/$preReleaseVersion/mono/Godot_v$version_mono_win32.zip",
                "extract_dir": "Godot_v$version_mono_win32"
            }
        },
        "hash": {
            "url": "$baseurl/SHA512-SUMS.txt"
        }
    }
}
